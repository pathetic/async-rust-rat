{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/16x16.png",
      "icons/32x32.png",
      "icons/192x192.png",
      "icons/512x512.png",
      "icons/icon.ico"
    ],
    "resources": [
      "../../res/countries.mmdb"
    ],
    "externalBin": []
  },
  "productName": "Async Rust RAT",
  "mainBinaryName": "Async Rust RAT",
  "version": "0.1.0",
  "identifier": "com.asyncrust.rat",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "RAT Server",
        "width": 1480,
        "height": 920,
        "useHttpsScheme": true
      }
    ]
  }
}
